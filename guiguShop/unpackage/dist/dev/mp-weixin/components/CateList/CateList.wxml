<view class="cateListContainer"><swiper class="banners" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{cateObj.category.bannerUrlList}}" wx:for-item="bannerItem" wx:for-index="index" wx:key="*this"><swiper-item><view class="swiper-item"><image class="bannerImg" src="{{bannerItem}}" mode></image></view></swiper-item></block></swiper><view class="title">{{cateObj.category.name}}</view><view class="desc">{{cateObj.category.frontDesc}}</view><view class="shopList"><block wx:for="{{cateObj.itemList}}" wx:for-item="shopItem" wx:for-index="index" wx:key="id"><view class="shopItem"><image class="shopImg" src="{{shopItem.listPicUrl}}" mode></image><view class="shopInfo common">{{shopItem.name}}</view><view class="shopPrice common">{{shopItem.retailPrice}}</view></view></block></view></view>